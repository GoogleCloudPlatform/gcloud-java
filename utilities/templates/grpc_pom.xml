<project xmlns="http://maven.apache.org/POM/4.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <artifactId>grpc-google-cloud-{{service}}-{{api_version}}</artifactId>
  <version>{{version}}</version><!-- {x-version-update:grpc-google-cloud-{{service}}-{{api_version}}:current} -->
  <name>grpc-google-cloud-{{service}}-{{api_version}}</name>
  <description>GRPC library for grpc-google-cloud-{{service}}-{{api_version}}</description>
  <parent>
    <groupId>com.google.api.grpc</groupId>
    <artifactId>google-api-grpc</artifactId>
    <version>{{parent_version}}</version><!-- {x-version-update:google-api-grpc:current} -->
  </parent>
  <dependencies>
    <dependency>
      <groupId>io.grpc</groupId>
      <artifactId>grpc-stub</artifactId>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>io.grpc</groupId>
      <artifactId>grpc-protobuf</artifactId>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.google.api.grpc</groupId>
      <artifactId>proto-google-cloud-{{service}}-{{api_version}}</artifactId>
      <scope>compile</scope>
    </dependency>
  </dependencies>

  <reporting>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-javadoc-plugin</artifactId>
        <version>3.1.0</version>
        <configuration>
          <show>protected</show>
          <nohelp>true</nohelp>
          <doclint>none</doclint>
          <offlineLinks>
            <offlineLink>
              <url>https://googleapis.dev/java/proto-google-cloud-{{service}}-{{api_version}/0.0.0/</url><!-- {x-version-update:proto-google-cloud-{{service}}-{{api_version}}:released} -->
              <location>../../../../proto-google-cloud-{{service}}-{{api_version}}/target/site/apidocs/</location>
            </offlineLink>
          </offlineLinks>
        </configuration>
      </plugin>
    </plugins>
  </reporting>
</project>
